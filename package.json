{
  "name": "letgo-automation",
  "version": "1.0.0",
  "description": "A basic example of automation with WebdriverIO, Cucumberjs & TypeScript.",
  "scripts": {
    "compile": "node ./node_modules/typescript/lib/tsc.js",
    "clean": "node ./node_modules/rimraf/bin.js tsrc",
    "test-local": "npm run compile && node ./node_modules/webdriverio/bin/wdio ./conf/local.conf.js",
    "test-remote": "npm run compile && node ./node_modules/webdriverio/bin/wdio ./conf/t.conf.js",
    "test:report": "allure generate ./report/allure-results --clean -o ./report/html& allure open ./report/html/"
  },
  "devDependencies": {},
  "dependencies": {
    "mime": "^1.4.0",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.42",
    "@types/webdriverio": "^4.4.0",
    "allure-commandline": "^2.3.4",
    "babel-jest": "~18.0.0",
    "babel-polyfill": "~6.20.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-register": "~6.18.0",
    "chai": "^3.5.0",
    "chai-webdriverio": "*",
    "eslint": "latest",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "~18.1.0",
    "rimraf": "^2.5.4",
    "ts-node": "^3.0.2",
    "typescript": "^2.5.1",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-browserstack-service": "0.1.11",
    "wdio-cucumber-framework": "latest",
    "wdio-cucumber-reporter": "latest",
    "wdio-junit-reporter": "^0.3.0",
    "wdio-selenium-standalone-service": "latest",
    "wdio-spec-reporter": "~0.0.5",
    "webdriverio": "^4.8.0"
  }
}
